isbn = {1106085108},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Biodiversity,Evolution,Gene Flow,Gene Flow: genetics,Genetic Speciation,Geography,Molecular,Molecular Sequence Data,Plants,Plants: genetics,Species Specificity},
month = aug,
number = {32},
pages = {13188--93},
pmid = {21730151},
title = {{Speciation with gene flow on Lord Howe Island.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3156170\&tool=pmcentrez\&rendertype=abstract http://www.ncbi.nlm.nih.gov/pubmed/21730151},
volume = {108},
