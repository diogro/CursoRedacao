month = oct,
number = {2},
pages = {621--32},
pmid = {21840853},
title = {{A new method to uncover signatures of divergent and stabilizing selection in quantitative traits.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3189809\&tool=pmcentrez\&rendertype=abstract},
volume = {189},
year = {2011}
}
