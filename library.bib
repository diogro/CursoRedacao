issn = {1545-7885},
journal = {PLoS biology},
keywords = {Animals,Evolution, Molecular,Genome,Genome, Human,Humans,Likelihood Functions,Pan troglodytes,Pan troglodytes: genetics,Polymerase Chain Reaction,Selection, Genetic,Zinc Fingers,Zinc Fingers: genetics},
