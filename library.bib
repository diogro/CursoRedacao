keywords = {Flowers,Flowers: anatomy \& histology,Flowers: genetics,Flowers: physiology,Reproduction,Reproduction: physiology,Selection, Genetic,Silene,Silene: anatomy \& histology,Silene: genetics,Silene: physiology},
month = oct,
