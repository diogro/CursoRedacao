keywords = {Analysis of Variance,Animals,Drosophila,Drosophila: genetics,Evolution, Molecular,Genetic Variation,Genetics, Population,Male,Models, Genetic,Selection, Genetic},
month = jun,
number = {1523},
pages = {1567--78},
pmid = {19414471},
title = {{Characterizing the evolution of genetic variance using genetic covariance tensors.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2691006\&tool=pmcentrez\&rendertype=abstract},
volume = {364},
