title = {{Multivariate mutation-selection balance with constrained pleiotropic effects.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1203687\&tool=pmcentrez\&rendertype=abstract},
volume = {122},
