pages = {1234--9},
pmid = {21220304},
title = {{Morphological change in machines accelerates the evolution of robust behavior.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3029735\&tool=pmcentrez\&rendertype=abstract},
volume = {108},
year = {2011}
}
