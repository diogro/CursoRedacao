month = jan,
number = {12},
pages = {e52204},
pmid = {23284934},
publisher = {Public Library of Science},
title = {{Environmental noise, genetic diversity and the evolution of evolvability and robustness in model gene networks.}},
url = {http://dx.plos.org/10.1371/journal.pone.0052204},
volume = {7},
year = {2012}
}
