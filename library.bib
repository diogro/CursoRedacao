issn = {1553-7358},
journal = {PLoS computational biology},
keywords = {Algorithms,Amino Acid Sequence,Archaea,Archaea: enzymology,Archaea: physiology,Bacteria,Bacteria: metabolism,Bacterial Physiological Phenomena,Bayes Theorem,Chitin,Chitin Synthase,Chitin Synthase: chemistry,Chitin: metabolism,Computational Biology,Databases, Genetic,Enzymes,Enzymes: chemistry,Eukaryotic Cells,Eukaryotic Cells: enzymology,Eukaryotic Cells: physiology,Metabolic Networks and Pathways,Models, Biological,Phylogeny,Signal Transduction},
