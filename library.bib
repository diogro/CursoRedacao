issn = {0016-6731},
journal = {Genetics},
keywords = {Alleles,Animals,Cell Lineage,Crosses, Genetic,Daphnia,Daphnia: genetics,Environment,Female,Gene Expression Regulation,Haploidy,Male,Meiosis,Models, Biological,Models, Genetic,Phylogeny,Sequence Analysis, DNA},
month = sep,
