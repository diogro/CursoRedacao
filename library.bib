@article{Pavlicev2009a,
abstract = {The concept of morphological integration describes the pattern and the amount of correlation between morphological traits. Integration is relevant in evolutionary biology as it imposes constraint on the variation that is exposed to selection, and is at the same time often based on heritable genetic correlations. Several measures have been proposed to assess the amount of integration, many using the distribution of eigenvalues of the correlation matrix. In this paper, we analyze the properties of eigenvalue vari- ance as a much applied measure. We show that eigenvalue variance scales linearly with the square of the mean cor- relation and propose the standard deviation of the eigenvalues as a suitable alternative that scales linearly with the correlation. We furthermore develop a relative measure that is independent of the number of traits and can thus be readily compared across datasets. We apply this measure to examples of phenotypic correlation matrices and compare our measure to several other methods. The relative standard deviation of the eigenvalues gives similar results as the mean absolute correlation (W.P. Cane, Evol Int J Org Evol 47:844â€“854, 1993) but is only identical to this measure if the correlation matrix is homogenous. For heterogeneous correlation matrices the mean absolute correlation is consistently smaller than the relative standard deviation of eigenvalues and may thus underestimate integration. Unequal allocation of variance due to variation among correlation coefficients is captured by the relative standard deviation of eigenvalues. We thus suggest that this measure is a better reflection of the overall morphological integration than the average correlation.},
author = {Pavlicev, Mihaela and Cheverud, James M and Wagner, G\"{u}nter P.},
doi = {10.1007/s11692-008-9042-7},
file = {:home/diogro/Dropbox/Mendeley//Pavlicev, Cheverud, Wagner - 2009 - Measuring Morphological Integration Using Eigenvalue Variance.pdf:pdf},
issn = {0071-3260},
journal = {Evolutionary Biology},
keywords = {constraint \'{a} phenotypic correlation,morphological integration \'{a} evolutionary,\'{a} eigenvalue},
month = feb,
number = {1},
pages = {157--170},
title = {{Measuring Morphological Integration Using Eigenvalue Variance}},
url = {Morphological integration; Evolutionary http://www.springerlink.com/index/10.1007/s11692-008-9042-7 http://link.springer.com/10.1007/s11692-008-9042-7},
volume = {36},
year = {2009}
}
