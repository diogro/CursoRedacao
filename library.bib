pages = {1506--24},
pmid = {22519787},
publisher = {Blackwell Publishing Inc},
title = {{Modularity, noise, and natural selection.}},
url = {http://doi.wiley.com/10.1111/j.1558-5646.2011.01555.x http://www.ncbi.nlm.nih.gov/pubmed/22519787 http://dx.doi.org/10.1111/j.1558-5646.2011.01555.x},
