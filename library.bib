pmid = {14766974},
title = {{Detection of multistability, bifurcations, and hysteresis in a large class of biological positive-feedback systems.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=357011\&tool=pmcentrez\&rendertype=abstract},
volume = {101},
year = {2004}
}
