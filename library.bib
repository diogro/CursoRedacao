pages = {1855--64},
pmid = {17507680},
title = {{Bayesian mapping of genomewide interacting quantitative trait loci for ordinal traits.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1931535\&tool=pmcentrez\&rendertype=abstract},
volume = {176},
