month = feb,
number = {7028},
pages = {895--900},
pmid = {15729348},
title = {{Functional cartography of complex metabolic networks}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2175124\&tool=pmcentrez\&rendertype=abstract http://www.nature.com/nature/journal/v433/n7028/abs/nature03288.html},
volume = {433},
