pmid = {17683427},
title = {{Expected relative fitness and the adaptive topography of fluctuating selection.}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1558-5646.2007.00170.x/full http://www.ncbi.nlm.nih.gov/pubmed/17683427},
volume = {61},
