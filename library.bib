journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Algorithms,Animal,Animals,Behavior,Biological Evolution,Computer Simulation,Extremities,Extremities: anatomy \& histology,Extremities: innervation,Neural Networks (Computer),Robotics,Robotics: methods},
month = jan,
