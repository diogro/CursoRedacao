journal = {Genetics},
keywords = {Animals,Computational Biology,Computational Biology: methods,Female,Genetic Markers,Genetic Markers: genetics,Genotype,Inheritance Patterns,Inheritance Patterns: genetics,Male,Mice,Models, Genetic,Mothers,Quantitative Trait Loci,Quantitative Trait Loci: genetics},
month = may,
number = {1},
pages = {261--77},
pmid = {22377636},
title = {{Detecting maternal-effect loci by statistical cross-fostering.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22377636},
volume = {191},
year = {2012}
