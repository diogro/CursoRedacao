keywords = {Animals,Dominant,Female,Genes,Heterozygote,Lod Score,Male,Mandible,Mandible: growth \& development,Mice,Morphogenesis,Morphogenesis: genetics,Phenotype,Quantitative Trait Loci,Quantitative Trait Loci: genetics,Sex Characteristics},
